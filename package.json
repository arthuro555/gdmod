{
  "name": "gdmod",
  "version": "1.1.0",
  "description": "A package containing a modding api for GDevelop games and a loader to load them.",
  "main": "./cli/cli.js",
  "scripts": {
    "gdmod-cli": "./cli/cli.js",
    "format": "prettier --write ./API/*.js ./cli/cli.js ./Loader/**/*.js \"./Mod Templates/**/*.js\" \"./Mod Templates/**/*.json\"",
	"check-format": "prettier --check ./API/*.js ./cli/cli.js ./Loader/**/*.js \"./Mod Templates/**/*.js\" \"./Mod Templates/**/*.json\""
  },
  "repository": {
    "type": "git",
    "url": "github.com/arthuro555/gdmod"
  },
  "keywords": [
    "mod",
    "modding",
    "GDevelop",
    "asar"
  ],
  "author": "Arthur Pacaud (arthuro555)",
  "license": "MIT",
  "dependencies": {
    "asar": "^3.0.3",
    "chalk": "^3.0.0",
    "commander": "^4.1.0"
  },
  "devDependencies": {
    "prettier": "^2.2.1"
  },
  "bin": {
    "gdmod-cli": "./cli/cli"
  }
}

{
  "name": "@gdmod/cli",
  "description": "A command line interface to do common tasks with GDMod, like patching a game.",
  "version": "0.2.2-test.0",
  "main": "./bin/index.js",
  "author": "Arthur Pacaud (arthuro555)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arthuro555/gdmod.git"
  },
  "scripts": {
    "start": "node ./scripts/watch.js",
    "build": "esbuild ./src/index.ts --outfile=bin/index.js --platform=node --target=node14 --bundle --minify --external:original-fs --external:@gdmod/api/dist/GDApi --external:@gdmod/local-loader --external:@fal-works/esbuild-plugin-global-externals --external:esbuild --external:jszip --external:inquirer --external:asar",
    "clean": "rm -r bin",
    "ts": "tsc"
  },
  "engines": {
    "node": ">=12.22.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.170",
    "chalk": "^3.0.0",
    "chokidar": "^3.5.1",
    "clipanion": "^3.0.0-rc.10",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "ora": "^5.4.0",
    "typescript": "^4.3.2"
  },
  "files": [
    "bin"
  ],
  "dependencies": {
    "@fal-works/esbuild-plugin-global-externals": "^2.1.1",
    "@gdmod/api": "^0.2.2-test.0",
    "@gdmod/local-loader": "^0.2.2-test.0",
    "asar": "^3.0.3",
    "esbuild": "^0.11.19",
    "inquirer": "^8.0.0",
    "jszip": "^3.6.0"
  },
  "bin": {
    "gdmod-cli": "./bin/index.js"
  },
  "publishConfig": {
    "access": "public"
  }
}

{
  "name": "@gdmod/api",
  "version": "0.2.1",
  "description": "Helper functions to mod GDevelop games and an API to create GDMod mods.",
  "main": "./src/index.ts",
  "module": "./dist/esm/index.js",
  "unpkg": "./dist/GDApi.js",
  "sideEffects": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arthuro555/gdmod.git"
  },
  "scripts": {
    "start": "node ./scripts/watch.cjs",
    "prepublish": "yarn build",
    "build": "node scripts/build.cjs",
    "ts": "node scripts/type-gen.cjs",
    "test": "jest",
    "docgen": "typedoc src/index.ts --name GDAPI --readme none --hideGenerator --excludePrivate --excludeInternal"
  },
  "author": "arthuro555",
  "license": "MIT",
  "dependencies": {
    "jszip": "^3.6.0",
    "path-browserify": "^1.0.1",
    "stream": "^0.0.2",
    "typanion": "^3.3.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "chokidar": "^3.5.1",
    "esbuild": "^0.8.55",
    "jest": "^26.6.3",
    "pixi.js": "^6.0.2",
    "ts-jest": "^26.5.6",
    "typedoc": "^0.20.34",
    "typescript": "4.2.4"
  },
  "browser": {
    "path": "path-browserify"
  },
  "publishConfig": {
    "access": "public"
  }
}
